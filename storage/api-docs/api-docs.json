{
    "openapi": "3.0.0",
    "info": {
        "title": "Mon API",
        "description": "Description de l'API",
        "version": "1.0.0"
    },
    "paths": {
        "/api/validationinscription/{token}": {
            "get": {
                "tags": [
                    "Utilisateur"
                ],
                "summary": "Valider l'email de l'utilisateur",
                "operationId": "1a7eb44068605aba0cfc6633550e1d00",
                "parameters": [
                    {
                        "name": "token",
                        "in": "path",
                        "description": "Token d'activation de l'utilisateur",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Utilisateur confirmé avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Utilisateur user@example.com a bien ete confirme"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Token invalide",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "success": {
                                            "type": "boolean",
                                            "example": false
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Token invalide"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur interne du serveur",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Une erreur interne s'est produite."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/utilisateurs": {
            "post": {
                "tags": [
                    "Utilisateur"
                ],
                "summary": "Créer un nouvel utilisateur",
                "operationId": "14e450b65dd82701eb3e7fcbcf6ce5af",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "nom",
                                    "mdp1",
                                    "mdp2"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user@example.com"
                                    },
                                    "nom": {
                                        "type": "string",
                                        "example": "Nom Utilisateur"
                                    },
                                    "mdp1": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    },
                                    "mdp2": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "password123"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Utilisateur créé avec succès",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Un e-mail de validation a été envoyé."
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Erreur de validation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Erreur de validation des données."
                                        },
                                        "messages": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Erreur interne du serveur",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Une erreur interne s'est produite."
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "Détails de l'erreur"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Utilisateur",
            "description": "Utilisateur"
        }
    ]
}